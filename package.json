{
  "name": "v4",
  "version": "1.0.0",
  "description": "Boilerplate with just TS and nothing else",
  "main": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html -p 9000 --open",
    "build": "parcel build src/index.html --out-dir dist --public-url './'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/leRenard/v-4.git"
  },
  "author": "Daniele Tabanella",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/leRenard/v-4/issues"
  },
  "homepage": "https://gitlab.com/leRenard/v-4#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "minimist": ">=1.2.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "sass": "^1.32.6",
    "typescript": "^4.1.2"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "dependencies": {
    "phaser": "^3.52.0"
  }
}
